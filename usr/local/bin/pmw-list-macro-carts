#!/bin/zsh

##############################################################################
#
# List macro carts: simply selects the CART number, TITLE and MACROS
# from Rivendell.CART.
#
##############################################################################

[[ -f ${ROOT:-/}usr/local/bin/zsh-functions ]] && source ${ROOT:-/}usr/local/bin/zsh-functions


doSQL "select NUMBER,TITLE,MACROS from CART where TYPE = 2"

exit
